BEGIN
    INTEGER I, PASS;
    BOOLEAN ARRAY DOORS(1:100);

    FOR I := 1 STEP 1 UNTIL 100 DO
        DOORS(I) := FALSE;

    FOR PASS := 1 STEP 1 UNTIL 100 DO
        FOR I := PASS STEP PASS UNTIL 100 DO
            DOORS(I) := NOT DOORS(I);

    FOR I := 1 STEP 1 UNTIL 100 DO
        IF DOORS(I)
          THEN BEGIN OUTTEXT("DOOR "); OUTINT(I,0); OUTTEXT(" IS OPEN");   OUTIMAGE END

END.
